<style>
html{position:relative; height:100%; background:#f2f2f2}
body{background:#f2f2f2}
html.sh_overflow{overflow:hidden;}

#mode_view {text-align:center; position:relative; height:100%}
#mode_view div.chkMode { width:100%; text-align:center; background:#fff; box-shadow:0 1px 2px rgba(0,0,0,0.2); z-index:999;}
#mode_view div.chkMode a{display:inline-block; position:relative; width:33.33%; max-width:80px; height:50px; vertical-align:top; border-left:1px solid #e0e0e0; background-position:50% 0; background-size:auto 100%; background-repeat:no-repeat; cursor:pointer; margin-left:-5px;}
#mode_view div.chkMode a.last{border-right:1px solid #ddd}
#mode_view div.chkMode a:hover,#mode_view div.chkMode a.ckmod{background-color:#f1f1f1;}
#mode_view div.chkMode a#mode_pc{background-image:url('/reactionImage/btn_mode_pc.png');}
#mode_view div.chkMode a#mode_laptop{background-image:url('/reactionImage/btn_mode_laptop.png');}
#mode_view div.chkMode a#mode_tablet{background-image:url('/reactionImage/btn_mode_tablet.png');}
#mode_view div.chkMode a#mode_mobile{background-image:url('/reactionImage/btn_mode_mobile.png');}

#mode_view div#frame_wrap {position:relative; margin:0 auto; overflow:hidden;}
#mode_view div#frame_wrap #sh_iframe{width:100%; margin:1px auto; position:relative; background:#fff}
#mode_view div#frame_wrap div.mode_bg{position:absolute; left:0; top:0; width:100%; text-align:center; opacity:0}


</style>

<div id="mode_view">

    
    <div class="chkMode">
        <a id="mode_pc" class="mode_pc ckmod" title="PC" style="opacity: 1;"></a>
        <a id="mode_laptop" class="mode_laptop" title="laptop" style="opacity: 1;"></a>
        <a id="mode_tablet" class="mode_tablet" title="tablet" style="opacity: 1;"></a>
        <a id="mode_mobile" class="mode_mobile last" title="mobile" style="opacity: 1;"></a>
    </div>
	
    <div id="frame_wrap" style="width: 100%; height: 874px; margin-top: 0px; margin-bottom: 0px; padding: 0px; overflow: hidden;">
        <div class="mode_bg mode_laptop" style="opacity: 0;"><img src="/reactionImage/mode_laptop.png"></div>
        <div class="mode_bg mode_tablet" style="opacity: 0;"><img src="/reactionImage/mode_tablet.png"></div>
        <div class="mode_bg mode_mobile" style="opacity: 0;"><img src="/reactionImage/mode_mobile.png"></div>
        
        <iframe width="100%" height="100%" id="sh_iframe" src="http://192.168.0.30:9000/mainTest" frameborder="0" border="0" marginwidth="0" marginheight="0" scrolling="yes"></iframe>
    </div>
    
</div>


<script>

$('.chkMode a').click(function(event){
	var modeid = $(this).attr('id');
	
	$('a').removeClass('ckmod');
	$('a.'+modeid).addClass('ckmod');

	$('html').removeClass('sh_overflow');
	
	if(modeid == 'mode_pc'){
		var winsize = $('html').height();
		var topbar = $('#mode_view .chkMode').outerHeight();
		var winheight = winsize-topbar;
		$('#frame_wrap').stop().animate({
			'width':'100%','height': winheight+'px',
			marginTop:'0',marginBottom:'0',
			paddingLeft:'0',paddingRight:'0',
			paddingTop:'0',paddingBottom:'0'
		});
		$('html').addClass('sh_overflow');
	}
	
	if(modeid == 'mode_laptop'){
		$('#frame_wrap').stop().animate({
			'width':'1042px','height':'770px',
			marginTop:'60px',marginBottom:'60px',
			paddingLeft:'8px',paddingRight:'11px',
			paddingTop:'56px',paddingBottom:'74px'
		},700);
	}
	
	if(modeid == 'mode_tablet'){
		$('#frame_wrap').stop().animate({
			'width':'800px','height':'1135px',
			marginTop:'60px',marginBottom:'60px',
			paddingLeft:'8px',paddingRight:'11px',
			paddingTop:'56px',paddingBottom:'74px'
		},700);
	}
	
	if(modeid == 'mode_mobile'){
		$('#frame_wrap').stop().animate({
			'width':'435px','height':'780px',
			marginTop:'60px',marginBottom:'60px',
			paddingLeft:'8px',paddingRight:'8px',
			paddingTop:'56px',paddingBottom:'63px'
		},700);
	}
	
	$('.mode_bg').stop().animate({opacity:'0'},300);
	$('.'+modeid).stop().animate({opacity:'1'},300);
	
	return false;
})

$('.chkMode a#mode_pc').trigger('click');
</script>



 




